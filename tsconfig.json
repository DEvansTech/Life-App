{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": [
      "es6"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "target": "esnext",
    "baseUrl": ".",
    "resolveJsonModule": true,
    "paths": {
      "@Assets": [
        "src/assets"
      ],
      "@Assets/*": [
        "src/assets/*"
      ],
      "@Components": [
        "src/components"
      ],
      "@Components/*": [
        "src/components/*"
      ],
      "@Context": [
        "src/context"
      ],
      "@Context/*": [
        "src/context/*"
      ],
      "@Hooks": [
        "src/hooks"
      ],
      "@Hooks/*": [
        "src/hooks/*"
      ],
      "@Navigators": [
        "src/navigators"
      ],
      "@Navigators/*": [
        "src/navigators/*"
      ],
      "@Redux": [
        "src/redux"
      ],
      "@Redux/*": [
        "src/redux/*"
      ],
      "@Screens": [
        "src/screens"
      ],
      "@Screens/*": [
        "src/screens/*"
      ],
      "@Theme": [
        "src/theme"
      ],
      "@Theme/*": [
        "src/theme/*"
      ]
    }
  },
  "filesGlob": [
    "typings/index.d.ts",
    "src/**/*.ts",
    "src/**/*.tsx"
  ],
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js",
    "test",
    "index.js"
  ],
}